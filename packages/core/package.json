{
  "name": "@stencil/core",
  "description": "A Compiler for Web Components and Progressive Web Apps",
  "author": "StencilJs Contributors",
  "homepage": "https://stenciljs.com/",
  "keywords": [
    "web components",
    "components",
    "stencil",
    "ionic",
    "webapp",
    "custom elements",
    "pwa",
    "progressive web app"
  ],
  "version": "5.0.0-next.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stenciljs/core.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "stencil": "./bin/stencil.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./compiler": {
      "types": "./dist/compiler/index.d.mts",
      "import": "./dist/compiler/index.mjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.mts",
      "import": "./dist/runtime/index.mjs"
    },
    "./runtime/client": {
      "types": "./dist/runtime/client/index.d.mts",
      "import": "./dist/runtime/client/index.mjs"
    },
    "./runtime/server": {
      "types": "./dist/runtime/server/index.d.mts",
      "import": "./dist/runtime/server/index.mjs"
    },
    "./runtime/app-data": "./dist/runtime/app-data/index.mjs",
    "./runtime/app-globals": "./dist/runtime/app-globals/index.mjs",
    "./compiler/utils": {
      "types": "./dist/compiler/utils/index.d.mts",
      "import": "./dist/compiler/utils/index.mjs"
    },
    "./sys/node": {
      "types": "./dist/sys/node/index.d.mts",
      "import": "./dist/sys/node/index.mjs"
    },
    "./testing": {
      "types": "./dist/testing/index.d.mts",
      "import": "./dist/testing/index.mjs"
    },
    "./mock-doc": {
      "import": "./dist/mock-doc.mjs"
    }
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/pluginutils": "^5.3.0",
    "@stencil/cli": "workspace:*",
    "@stencil/mock-doc": "workspace:*",
    "autoprefixer": "^10.4.24",
    "chalk": "^5.6.2",
    "css-what": "^7.0.0",
    "glob": "^10.0.0",
    "graceful-fs": "^4.2.11",
    "magic-string": "^0.30.0",
    "merge-source-map": "^1.1.0",
    "minimatch": "^10.1.2",
    "postcss": "^8.5.6",
    "postcss-safe-parser": "^7.0.1",
    "postcss-selector-parser": "^7.1.1",
    "resolve": "^1.22.0",
    "rollup": "^4.0.0",
    "semver": "^7.7.4",
    "terser": "5.37.0",
    "typescript": "~5.8.3"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "devDependencies": {
    "@ionic/prettier-config": "^4.0.0",
    "@stencil/vitest": "^1.1.14",
    "vitest-environment-stencil": "^1.1.14"
  }
}
